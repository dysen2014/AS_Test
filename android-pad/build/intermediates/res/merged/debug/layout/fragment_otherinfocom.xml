<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bookbuildingbg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="84dp"
        android:gravity="center" >

        <LinearLayout
            android:layout_width="410dp"
            android:layout_height="36dp"
            android:layout_gravity="center"
            android:background="@drawable/workplaceselectorshape" >

            <RelativeLayout
                android:id="@+id/rl_bookbuildingbaseinfo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="基础信息"
                    android:textColor="@color/nowworkplacewordbg"
                    android:textSize="17sp" />
            </RelativeLayout>

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/nowworkplacewordbg" />

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="经营状况信息"
                    android:textColor="@color/nowworkplacewordbg"
                    android:textSize="17sp" />
            </RelativeLayout>

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/nowworkplacewordbg" />

            <RelativeLayout
                android:id="@+id/rl_bookbuildingother"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/itembg5" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="其他信息"
                    android:textColor="@color/white"
                    android:textSize="17sp" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bookbuildingbg"
        android:divider="@drawable/gray_divider"
        android:orientation="vertical"
        android:showDividers="middle" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:divider="@drawable/gray_divider"
            android:orientation="horizontal"
            android:showDividers="middle" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="注册资本："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="注册地址："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:divider="@drawable/gray_divider"
            android:orientation="horizontal"
            android:showDividers="middle" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="贷款卡号："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="中征码："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:divider="@drawable/gray_divider"
            android:orientation="horizontal"
            android:showDividers="middle" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="国税税务登记代码："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info5"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="地税税务登记代码："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info6"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:divider="@drawable/gray_divider"
            android:orientation="horizontal"
            android:showDividers="middle" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="其他证件："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info7"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="机构信用代码证（人行专用）："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info8"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:divider="@drawable/gray_divider"
            android:orientation="horizontal"
            android:showDividers="middle" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="营业执照："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info9"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="center_vertical|right"
                android:text="组织机构代码证："
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </TextView>

            <EditText
                android:id="@+id/et_info10"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:background="@null"
                android:paddingLeft="4dp"
                android:text=""
                android:textColor="#4d4d4d"
                android:textSize="16.5sp" >
            </EditText>
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/btn_otherinfonext"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="45dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/cusarchiving_step_next" />

</LinearLayout>